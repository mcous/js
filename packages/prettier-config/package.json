{
  "name": "@mcous/prettier-config",
  "version": "0.1.0",
  "description": "My base Prettier configuration.",
  "type": "commonjs",
  "exports": {
    "types": "./dist/base.d.ts",
    "default": "./dist/base.js"
  },
  "main": "./dist/base.js",
  "types": "./dist/base.d.ts",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
