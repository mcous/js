  "keywords": [
    "prettier-config"
  ],
  "author": "Michael Cousins <michael@cousins.io> (https://mike.cousins.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcous/js.git",
    "directory": "packages/prettier-config"
  },
  "bugs": {
    "url": "https://github.com/mcous/js/issues"
  },
  "homepage": "https://github.com/mcous/js/tree/main/packages/prettier-config#readme",
  "eslintConfig": {
    "rules": {
      "unicorn/prefer-module": "off"
    }
  },
  "peerDependencies": {
    "prettier": ">=2.0.0 <3"
  }
}
